<h1> New Registered Application </h1>

<%= form_for @registered_application do |f| %>
 <div class="form-group">
   <%= f.label :name %>
   <%= f.text_field :name, class: 'form-control', placeholder: "Enter application name" %>
 </div>
 <div class="form-group">
   <%= f.label :URL %>
   <%= f.text_area :URL, rows: 8, class: 'form-control', placeholder: "Enter application URL" %>
 </div>
 <%= f.submit "Save", class: 'btn btn-success' %>
<% end %>